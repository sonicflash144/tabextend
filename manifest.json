{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Save and manage your tabs like TabExtend",
  "permissions": ["tabs", "storage", "activeTab", "contextMenus"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}