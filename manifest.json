{
  "manifest_version": 3,
  "name": "Tabs Magic",
  "version": "1.0.16.7",
  "description": "Save and manage your tabs into visual columns.",
  "permissions": ["tabs", "tabGroups", "storage", "activeTab", "contextMenus"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}